   keywords = {Autonomous aerial vehicles,Downlink,Information processing,Internet of Things,Internet-of-Things (IoT),Optimization,Protocols,RIS,UAV,Uplink,deep reinforcement learning,wireless power transfer},
   month = {8},
   pages = {1086-1096},
