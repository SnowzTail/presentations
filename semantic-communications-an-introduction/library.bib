@inproceedings{Yang2019a,
   abstract = {Intelligent reflecting surface (IRS) is a promising new technology for achieving spectrum and energy efficient wireless communication systems in the future. By adaptively varying the incident signals' phases/amplitudes and thereby establishing favorable channel responses through a large number of reconfigurable passive reflecting elements, IRS is able to enhance the communication performance of mobile users in its vicinity cost- effectively. In this paper, we study an IRS- enhanced orthogonal frequency division multiplexing (OFDM) system in which an IRS is deployed to assist the communication between a nearby user and its associated base station (BS). We aim to maximize the downlink achievable rate for the user by jointly optimizing the transmit power allocation at the BS and the passive array reflection coefficients at the IRS. Although the formulated problem is non-convex and thus difficult to solve, we propose an efficient algorithm to obtain a high-quality suboptimal solution for it, by alternately optimizing the BS's power allocation and the IRS's passive array coefficients in an iterative manner, along with a customized method for the initialization. Simulation results show that the proposed design significantly improves the OFDM link rate performance as compared to the cases without the IRS or with other heuristic IRS designs.},
   author = {Yifei Yang and Shuowen Zhang and Rui Zhang},
   doi = {10.1109/GLOBECOM38437.2019.9014204},
   isbn = {978-1-7281-0962-6},
   journal = {2019 IEEE Global Communications Conference (GLOBECOM)},
   keywords = {Intelligent reflecting surface (IRS),OFDM,Passive array optimization,Power allocation},
   month = {12},
