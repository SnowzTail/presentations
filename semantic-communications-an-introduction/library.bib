   journal = {IEEE Internet of Things Journal},
   keywords = {Aging,Ambient backscatter,Backscatter,Binary phase shift keying,OFDM,Receivers,Receiving antennas,Signal to noise ratio,coherent detection,machine learning,noncoherent detection,performance analysis.},
   pages = {1-1},
   title = {Coherent Multi-Antenna Receiver for BPSK-Modulated Ambient Backscatter Tags},
