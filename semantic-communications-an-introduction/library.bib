   keywords = {Intelligent reflecting surface,multiuser multiple-input single-output (MISO),resource allocation,wireless-powered communication networks},
   month = {12},
   pages = {8399-8410},
   publisher = {Institute of Electrical and Electronics Engineers Inc.},
   title = {Joint Beamforming and Power Control for Throughput Maximization in IRS-assisted MISO WPCNs},
