   issn = {2644-125X},
   journal = {IEEE Open Journal of the Communications Society},
   keywords = {Backscattering,beam-focusing,beam-steering,electromagnetic wave control,far-field,intelligent reflecting surfaces,near-field,spatial modulation},
   pages = {1256-1266},
   publisher = {Institute of Electrical and Electronics Engineers Inc.},
   title = {Intelligent Reflecting Surfaces With Spatial Modulation: An Electromagnetic Perspective},
   volume = {1},
   url = {https://ieeexplore.ieee.org/document/9171362/},
   year = {2020},
}
