   keywords = {Energy harvesting,ergodic capacity,fading channel,outage probability,power control,wireless power transfer},
   month = {1},
   pages = {288-300},
   publisher = {IEEE},
   title = {Wireless Information Transfer With Opportunistic Energy Harvesting},
