   journal = {IEEE Transactions on Wireless Communications},
   keywords = {Backscatter,Backscatter communications,Benchmark testing,Downlink,NOMA,Relays,Signal to noise ratio,Wireless communication,power-domain NOMA,relay,user cooperation},
   month = {6},
   pages = {3458-3474},
   title = {Backscatter Cooperation in NOMA Communications Systems},
   volume = {20},
   url = {https://ieeexplore.ieee.org/document/9328169/},
   year = {2021},
}
