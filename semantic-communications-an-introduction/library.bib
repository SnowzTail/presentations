   keywords = {Ambient backscatter,Bit error rate,Green communications,Internet of Things,Reconfigurable antennas,Software-defined radio},
   month = {12},
   pages = {1-6},
   publisher = {IEEE},
   title = {First Experimental Ambient Backscatter Communication Using a Compact Reconfigurable Tag Antenna},
   url = {https://ieeexplore.ieee.org/document/9024698/},
   year = {2019},
}
